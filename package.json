{
  "name": "moonlight-utils",
  "version": "1.0.11",
  "description": "moonlight-utils is a javaScript utility library",
  "main": "dist/index.js",
  "scripts": {
    "build:dev": "webpack --inline --progress --hide-modules",
    "start": "webpack-dev-server --inline --progress --hide-modules --open",
    "build:tsc": "tsc",
    "build:lib": "webpack --progress --hide-modules --config webpack.npm.config.js",
    "package": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.npm.config.js",
    "test": "npm run build:lib && mocha --reporter spec",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha test/*.js - - -R spec",
    "publish:pkg": "npm run package && npm publish --registry=https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JerryYu2014/moonlight-utils.git"
  },
  "keywords": [
    "javascript",
    "utils"
  ],
  "author": "advoster",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JerryYu2014/moonlight-utils/issues"
  },
  "homepage": "https://github.com/JerryYu2014/moonlight-utils#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@types/clipboard": "^2.0.1",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "html-webpack-plugin": "^4.3.0",
    "istanbul": "^0.4.5",
    "mocha": "^8.0.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@types/node": "^14.0.26",
    "clipboard": "^2.0.6",
    "moonlight-algorithm": "^1.0.1",
    "vue": "^2.6.11"
  }
}
